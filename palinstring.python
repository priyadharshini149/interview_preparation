class Solution(object):
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        st=""
        for i in s:
            if(i.isalnum()):
                st+=i
        st1=st.lower();
        s1=""
        for i in st1:
            s1=i+s1
        return st1==s1
        