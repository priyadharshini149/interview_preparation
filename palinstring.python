class Solution(object):
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        st=""
        for i in s:
            if(i.isalnum()):
                st+=i
        st1=st.lower();
        str=""
        for i in st1:
            str=i+str
        return st1==str
        
